{
  "database": {
    "rules": "database.rules.json"
  },
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "hosting": {
    "site": "sciencecookies",
    "public": "public",
    "cleanUrls": true,
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/galletas/*/*",
        "function": "dynCookies-showCookie"
      },
      {
        "source": "/editar/*",
        "function": "dynCookies-showCookie"
      },
      {
        "source": "/vista-previa/*",
        "function": "dynCookies-showCookie"
      },
      {
        "source": "/vista-email/*",
        "function": "dynCookies-showCookie"
      }
    ]
  },
  "storage": {
    "rules": "storage.rules"
  },
  "emulators": {
    "functions": {
      "port": 5001
    },
    "firestore": {
      "port": 8080
    },
    "database": {
      "port": 9000
    },
    "hosting": {
      "port": 5000
    },
    "pubsub": {
      "port": 8085
    },
    "ui": {
      "enabled": true
    }
  }
}